package com.zhiyou100.day06;

public class Test2 {

	public static void main(String[] args) {
		int[] arr= {0,1,1,4,-7,9,11};
		 //ц╟ещеепР
		 for (int i = 0; i < arr.length-1; i++) {
			   for (int j = 0; j < arr.length-i-1; j++) {
				     System.out.print(arr[j]+"\t");
			   }
			   System.out.println();
		 }

	}

}
