package com.zhiyou100.day06;

public class PaiXu2_MaoPao {
	public static void main(String[] args) {
		 //冒泡排序：依次拿相邻的元素做比较
		 int[] arr= {1,5,8,0,9,1};
		 for (int i = 0; i < arr.length-1; i++) {
			   for (int j = 0; j < arr.length-i-1; j++) {
				     System.out.print("arr["+j+"]--arr["+(j+1)+"]");
			   }
		 }
		   
		
	}
}
