package com.zhiyou100.day10;

public class LianXi2 {

	
    
    public static void main(String[] args) {
        
    }
    //黑洞
    public static void heiDong() {
    	//定义变量 让其把所有的四位数跑完
        for (int n = 1000; n < 10000; n++) {
			//排除四个位数完全相同：1111 2222 3333 
        	if(n%1111==0) {
        		continue;//继续n++ 
        	}
        	//求n进行黑洞的次数
        	int ciShu=0;
        	
		}
    }
    
    
    
    
   
}
