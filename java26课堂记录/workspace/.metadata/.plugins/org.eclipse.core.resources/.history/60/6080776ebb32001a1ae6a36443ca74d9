package com.zhiyou100.day06;

public class Test2 {

	public static void main(String[] args) {
		int[] arr= {0,1,1,4,-7,9,11};
		 //冒泡排序
		 for (int i = 0; i < arr.length-1; i++) {
			   for (int j = 0; j < arr.length-i-1; j++) {
				     //拿arr[j]和arr[j+1]做比较
				     if(arr[j]<arr[j+1]) {
				    	 int k=arr[j];arr[j]=arr[j+1];arr[j+1]=k;
				     }
			   }
		 }

	}

}
