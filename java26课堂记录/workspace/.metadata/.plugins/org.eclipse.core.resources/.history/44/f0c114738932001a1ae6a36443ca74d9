package com.zhiyou100.day06;

public class PaiXu1_ShunXu {

	public static void main(String[] args) {
		int[] arr= {1,5,1,9,0,3};
		for (int i = 0; i < arr.length-1; i++) {
			for (int j = i+1; j < arr.length; j++) {
				System.out.print("arr["+i+"]--arr["+j+"]\t");
			}
			System.out.println();
		}

	}

}
