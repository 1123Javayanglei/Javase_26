package com.zhiyou100.day08;
public class Demo1_OOP4_OverLoad {

	public static void main(String[] args) {
		  Student  s1=new Student();
		  int sum1=s1.add(1, 2);
		  sum1=s1.add(1, 1.1);
		  System.out.println("sum1="+sum1);
	      /*
	       * 方法重载：
	       * 
	       * */
		
	}
}
class  Student{
	//求和：求两个int的和
	public int  add(int a,int b) {
		return a+b;
	}
} 
