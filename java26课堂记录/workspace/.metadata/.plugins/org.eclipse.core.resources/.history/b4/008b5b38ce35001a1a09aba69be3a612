package com.zhiyou100.day08;
public class Demo1_OOP5_Constructor {

	public static void main(String[] args) {
		//创建对象
		Teacher t1=new Teacher();
		//给对象的属性赋值
		t1.age=28;
		t1.name="韩梅梅";
		//调用对象的方法
		t1.show();
		
	}
}
class  Teacher{
	int age=29;
	String name;
	public void show() {
		System.out.println("age="+age+",name="+name);
	}
} 
